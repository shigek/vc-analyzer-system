{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "resolver": {
      "type": "application",
      "root": "apps/resolver",
      "entryFile": "main",
      "sourceRoot": "apps/resolver/src",
      "compilerOptions": {
        "tsConfigPath": "apps/resolver/tsconfig.app.json"
      }
    },
    "share": {
      "type": "library",
      "root": "libs/share",
      "entryFile": "index",
      "sourceRoot": "libs/share/src",
      "compilerOptions": {
        "tsConfigPath": "libs/share/tsconfig.lib.json"
      }
    },
    "status-list": {
      "type": "application",
      "root": "apps/status-list",
      "entryFile": "main",
      "sourceRoot": "apps/status-list/src",
      "compilerOptions": {
        "tsConfigPath": "apps/status-list/tsconfig.app.json"
      }
    },
    "trusted-list": {
      "type": "application",
      "root": "apps/trusted-list",
      "entryFile": "main",
      "sourceRoot": "apps/trusted-list/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trusted-list/tsconfig.app.json"
      }
    },
    "vc-analyzer-system": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    }
  }
}